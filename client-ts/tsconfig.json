{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "es7",
            "dom"
        ],
        "types": [
            "webpack-env"
        ],
        "baseUrl": "./src/",
        "rootDir": "./src/",
        "paths": {
            "*": ["*", "./src/*"]
        },
        "jsx": "react",
        "strict": true,
        "noEmitOnError": true
    },
    "exclude": [
        "node_modules"
    ]
}