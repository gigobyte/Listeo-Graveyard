    "start-webpack": "webpack -w --config webpack.config.js",
    "start-webpack-prod": "webpack -w --config webpack.config.production.js",
    "start-express": "node ./main",
    "start": "SET NODE_ENV=dev && setup && concurrently --no-color -k \"mongod\" \"npm run start-webpack\" \"npm run start-express\"",
    "start-prod": "SET NODE_ENV=production && setup && concurrently --no-color -k \"mongod\" \"npm run start-webpack-prod\" \"npm run start-express\"",
    "lint": "eslint client-src/**/*.jsx client-src/**/*.js server/**/*",
    "test": "mocha --opts mocha.opts",
    "test-coverage": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "clean": "rm -rf client-dist coverage"
  },
